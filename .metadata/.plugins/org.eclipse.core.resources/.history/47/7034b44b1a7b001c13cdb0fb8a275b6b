public class ExecuteDebug {
	
	public static void main(String[] args) {
		// VOU USAR O EXEMPLO DE UMA CALCULADORA PARA DEBUG
		
		soma(2,10);
		
	}
	
	
	// CRIANDO UMA CALCULADORA QUE SOMA DOIS NUMEROS
	
	public static int soma(int a, int b) {
		
		return a + b;
	}

}
